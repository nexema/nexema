    "version": "0.2.0",
    "configurations": [
        {
            "name": "generate",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["generate", "--for=js=./example/out/js", "./integration_test/sample-project"],
            "program": "${workspaceFolder}"
        },
        {
            "name": "build",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["build", "./example"],
            "program": "${workspaceFolder}"
        },
        {
            "name": "plugin install",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["plugin", "install", "js"],
            "program": "${workspaceFolder}"
        },
        {
            "name": "plugin list",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["plugin", "list"],
            "program": "${workspaceFolder}"
        }
    ]
}