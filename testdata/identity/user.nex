import:
    "."

// Primary user
type User struct {
    id: string
    name: string
    selected_color: Colors
    account: Account?
    tags: list(string)
    claims: map(string,string?)
}

type Account union {
    admin: AdminAccount
    customer: CustomerAccount
}

type AdminAccount struct {
    active: bool
}

type CustomerAccount struct {
    age: uint8?
}